<?xml version='1.0' encoding='utf-8'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jadn="jadn_base_types">
	<xs:import schemaLocation="../_data/xsd/jadn_base_types.xsd" namespace="jadn_base_types" />
	<xs:complexType name="OpenC2-Command">
		<xs:sequence>
			<xs:element name="action" type="Action" />
			<xs:element name="target" type="Target" />
			<xs:element name="args" type="Args" minOccurs="0" />
			<xs:element name="actuator" type="Actuator" minOccurs="0" />
			<xs:element name="command_id" type="Command-ID" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OpenC2-Response">
		<xs:sequence>
			<xs:element name="OpenC2-Response-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="status" type="Status-Code" />
						<xs:element name="status_text" type="jadn:String" minOccurs="0" />
						<xs:element name="results" type="Results" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Action">
		<xs:restriction base="xs:string">
			<xs:enumeration value="query" />
			<xs:enumeration value="investigate" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Target">
		<xs:choice>
			<xs:element name="features" type="Features" />
			<xs:element name="th" type="AP-Target" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Args">
		<xs:sequence>
			<xs:element name="Args-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="start_time" type="Date-Time" minOccurs="0" />
						<xs:element name="stop_time" type="Date-Time" minOccurs="0" />
						<xs:element name="duration" type="Duration" minOccurs="0" />
						<xs:element name="response_requested" type="Response-Type" minOccurs="0" />
						<xs:element name="th" type="AP-Args" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Actuator">
		<xs:choice>
			<xs:element name="th" type="AP-Specifiers" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="Results">
		<xs:annotation>
			<xs:documentation>Response Results</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Results-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="versions" type="Version" minOccurs="0" maxOccurs="10" />
						<xs:element name="profiles" type="Nsid" minOccurs="0" maxOccurs="unbounded" />
						<xs:element name="pairs" type="Pairs" minOccurs="0" />
						<xs:element name="rate_limit" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="jadn:Number">
									<xs:minInclusive value="0.0" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="args" type="jadn:String" minOccurs="0" />
						<xs:element name="th" type="AP-Results" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Pairs">
		<xs:restriction base="xs:string">
			<xs:enumeration value="query: features, /huntflows, /datasources" />
			<xs:enumeration value="investigate: /hunt" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AP-Target">
		<xs:annotation>
			<xs:documentation>TH targets defined in this profile.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="hunt" type="jadn:String" />
			<xs:element name="huntflows" type="Huntflow-Specifiers" />
			<xs:element name="datasources" type="jadn:String" minOccurs="0" />
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="AP-Args">
		<xs:annotation>
			<xs:documentation>TH command arguments defined in this profile.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AP-Args-Elements" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="huntargs" type="Huntargs" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Huntargs">
		<xs:sequence>
			<xs:element name="string_args" type="jadn:String" minOccurs="0" />
			<xs:element name="integer_args" type="jadn:Integer" minOccurs="0" />
			<xs:element name="typed_args" type="Typed-Arguments" minOccurs="0" />
			<xs:element name="native_oc2" type="OC2-Data" minOccurs="0" />
			<xs:element name="stix" type="STIX-Cybersecurity-Observables" minOccurs="0" />
			<xs:element name="stix_extensions" type="jadn:String" minOccurs="0" />
			<xs:element name="timeranges" type="Timeranges" minOccurs="0" />
			<xs:element name="datasources" type="Datasource-Array" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OC2-Data">
		<xs:annotation>
			<xs:documentation>OC2-Data is an array of one or more types defined in the OpenC2 language spec</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="OC2-Data-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Language-Spec-Types" type="Language-Spec-Types" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Language-Spec-Types">
		<xs:sequence>
			<xs:element name="artifact" type="Artifact" minOccurs="0" />
			<xs:element name="device" type="Device" minOccurs="0" />
			<xs:element name="domain_name" type="Domain-Name" minOccurs="0" />
			<xs:element name="email-address" type="Email-Addr" minOccurs="0" />
			<xs:element name="file" type="File" minOccurs="0" />
			<xs:element name="hashes" type="Hashes" minOccurs="0" />
			<xs:element name="hostname" type="Hostname" minOccurs="0" />
			<xs:element name="idn_domain_name" type="IDN-Domain-Name" minOccurs="0" />
			<xs:element name="idn_email_address" type="IDN-Email-Addr" minOccurs="0" />
			<xs:element name="ipv4_address" type="IPv4-Addr" minOccurs="0" />
			<xs:element name="ipv6_address" type="IPv6-Addr" minOccurs="0" />
			<xs:element name="ipv4_network" type="IPv4-Net" minOccurs="0" />
			<xs:element name="ipv6_network" type="IPv6-Net" minOccurs="0" />
			<xs:element name="ipv4_connection" type="IPv4-Connection" minOccurs="0" />
			<xs:element name="ipv6_connection" type="IPv6-Connection" minOccurs="0" />
			<xs:element name="iri" type="IRI" minOccurs="0" />
			<xs:element name="mac_address" type="MAC-Addr" minOccurs="0" />
			<xs:element name="port" type="Port" minOccurs="0" />
			<xs:element name="process" type="Process" minOccurs="0" />
			<xs:element name="uri" type="URI" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Artifact">
		<xs:sequence>
			<xs:element name="mime_type" type="jadn:String" minOccurs="0" />
			<xs:element name="payload" type="Payload" minOccurs="0" />
			<xs:element name="hashes" type="Hashes" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Device">
		<xs:sequence>
			<xs:element name="Device-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="hostname" type="Hostname" minOccurs="0" />
						<xs:element name="idn_hostname" type="IDN-Hostname" minOccurs="0" />
						<xs:element name="device_id" type="jadn:String" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Domain-Name">
		<xs:annotation>
			<xs:documentation>[[RFC1034]](#rfc1034), Section 3.5</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:hostname">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.3</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Email-Addr">
		<xs:annotation>
			<xs:documentation>Email address, [[RFC5322]](#rfc5322), Section 3.4.1</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:email">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.2</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="File">
		<xs:sequence>
			<xs:element name="File-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="name" type="jadn:String" minOccurs="0" />
						<xs:element name="path" type="jadn:String" minOccurs="0" />
						<xs:element name="hashes" type="Hashes" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IDN-Domain-Name">
		<xs:annotation>
			<xs:documentation>Internationalized Domain Name, [[RFC5890]](#rfc5890), Section 2.3.2.3.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:idn-hostname">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.3</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDN-Email-Addr">
		<xs:annotation>
			<xs:documentation>Internationalized email address, [[RFC6531]](#rfc6531)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:idn-email">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.2</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="IPv4-Net">
		<xs:sequence>
			<xs:element name="IPv4-Net-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ipv4_addr" type="IPv4-Addr" />
						<xs:element name="prefix_length" type="jadn:Integer" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IPv4-Connection">
		<xs:sequence>
			<xs:element name="src_addr" type="IPv4-Net" minOccurs="0" />
			<xs:element name="src_port" type="Port" minOccurs="0" />
			<xs:element name="dst_addr" type="IPv4-Net" minOccurs="0" />
			<xs:element name="dst_port" type="Port" minOccurs="0" />
			<xs:element name="protocol" type="L4-Protocol" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IPv6-Net">
		<xs:sequence>
			<xs:element name="IPv6-Net-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ipv6_addr" type="IPv6-Addr" />
						<xs:element name="prefix_length" type="jadn:Integer" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IPv6-Connection">
		<xs:sequence>
			<xs:element name="src_addr" type="IPv6-Net" minOccurs="0" />
			<xs:element name="src_port" type="Port" minOccurs="0" />
			<xs:element name="dst_addr" type="IPv6-Net" minOccurs="0" />
			<xs:element name="dst_port" type="Port" minOccurs="0" />
			<xs:element name="protocol" type="L4-Protocol" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="IRI">
		<xs:annotation>
			<xs:documentation>Internationalized Resource Identifier, [[RFC3987]](#rfc3987).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:iri">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.5</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MAC-Addr">
		<xs:annotation>
			<xs:documentation>Media Access Control / Extended Unique Identifier address - EUI-48 or EUI-64 as defined in [[EUI]](#eui).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:eui">
			<xs:annotation>
				<xs:documentation>IEEE Extended Unique Identifier (MAC Address)</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Process">
		<xs:sequence>
			<xs:element name="Process-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="pid" minOccurs="0">
							<xs:simpleType>
								<xs:restriction base="jadn:Integer">
									<xs:minInclusive value="0" />
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
						<xs:element name="name" type="jadn:String" minOccurs="0" />
						<xs:element name="cwd" type="jadn:String" minOccurs="0" />
						<xs:element name="executable" type="File" minOccurs="0" />
						<xs:element name="parent" type="Process" minOccurs="0" />
						<xs:element name="command_line" type="jadn:String" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="URI">
		<xs:annotation>
			<xs:documentation>Uniform Resource Identifier, [[RFC3986]](#rfc3986).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:uri">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.5</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Hashes">
		<xs:annotation>
			<xs:documentation>Cryptographic Hash values</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Hashes-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="md5" type="jadn:Binary" minOccurs="0" />
						<xs:element name="sha1" type="jadn:Binary" minOccurs="0" />
						<xs:element name="sha256" type="jadn:Binary" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Hostname">
		<xs:annotation>
			<xs:documentation>Internet host name as specified in [[RFC1123]](#rfc1123)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:hostname">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.3</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IDN-Hostname">
		<xs:annotation>
			<xs:documentation>Internationalized Internet host name as specified in [[RFC5890]](#rfc5890), Section 2.3.2.3.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:idn-hostname">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.3</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IPv4-Addr">
		<xs:annotation>
			<xs:documentation>32 bit IPv4 address as defined in [[RFC0791]](#rfc0791)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:ipv4-addr">
			<xs:annotation>
				<xs:documentation>IPv4 address as specified in RFC 791 Section 3.1</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IPv6-Addr">
		<xs:annotation>
			<xs:documentation>128 bit IPv6 address as defined in [[RFC8200]](#rfc8200)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:ipv6-addr">
			<xs:annotation>
				<xs:documentation>IPv6 address as specified in RFC 8200 Section 3</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Port">
		<xs:annotation>
			<xs:documentation>Transport Protocol Port Number, [[RFC6335]](#rfc6335)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:Integer">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="65535" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AP-Specifiers">
		<xs:annotation>
			<xs:documentation>TH actuator specifiers (may be empty).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AP-Specifiers-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence />
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Huntflow-Specifiers">
		<xs:annotation>
			<xs:documentation>TH Huntflow specifiers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huntflow-Specifiers-Elements" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="path" type="jadn:String" minOccurs="0" />
						<xs:element name="tags" type="Tags" minOccurs="0" />
						<xs:element name="arg_types" type="Specified-Arg-Types" minOccurs="0" />
						<xs:element name="arg_names" type="Specified-Arg-Names" minOccurs="0" />
						<xs:element name="format_types" type="Return-Type" minOccurs="0" />
						<xs:element name="return_format" type="Huntflow-Sections" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Specified-Arg-Types">
		<xs:annotation>
			<xs:documentation>Return huntflows that take these argument types.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Specified-Arg-Types-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Arg-Type" type="Arg-Type" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Specified-Arg-Names">
		<xs:annotation>
			<xs:documentation>Return huntflows that take arguments with these names.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Specified-Arg-Names-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Arg-Name" type="Arg-Name" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AP-Results">
		<xs:annotation>
			<xs:documentation>TH results defined in this profile.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="AP-Results-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="huntflow_info" type="Huntflow-Info" minOccurs="0" />
						<xs:element name="datasources" type="Datasource-Array" minOccurs="0" />
						<xs:element name="inv_returns" type="Inv-Returns" minOccurs="0" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Inv-Returns">
		<xs:annotation>
			<xs:documentation>Array of returns from threat hunt investigations.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Inv-Returns-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Inv-Return" type="Inv-Return" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Inv-Return">
		<xs:sequence>
			<xs:element name="string_returns" type="jadn:String" minOccurs="0" />
			<xs:element name="stix_sco" type="STIX-Cybersecurity-Observables" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Timeranges">
		<xs:annotation>
			<xs:documentation>a timerange used in the execution of a hunt.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Timeranges-Elements" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Timerange" type="Timerange" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Timerange">
		<xs:annotation>
			<xs:documentation>Identification of process to be targeted by Threat Hunting activity.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="timerange_absolute" type="Timerange-Abs" minOccurs="0" />
			<xs:element name="timerange_relative" type="Timerange-Rel" minOccurs="0" />
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="Time-Unit">
		<xs:annotation>
			<xs:documentation>Time Unit Keywords.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Days" />
			<xs:enumeration value="Hours" />
			<xs:enumeration value="Minutes" />
			<xs:enumeration value="Seconds" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Timerange-Abs">
		<xs:sequence>
			<xs:element name="hunt_start_time" type="timestamp" />
			<xs:element name="hunt_stop_time" type="timestamp" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Timerange-Rel">
		<xs:sequence>
