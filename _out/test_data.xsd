<?xml version='1.0' encoding='utf-8'?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jadn="jadn_base_types">
	<xs:import schemaLocation="../_data/xsd/jadn_base_types.xsd" namespace="jadn_base_types" />
	<xs:complexType name="Library">
		<xs:sequence>
			<xs:element name="Library-Elements" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element id="library_library" name="Barcode" />
						<xs:element id="library_library" name="Album" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Barcode">
		<xs:annotation>
			<xs:documentation>A UPC-A barcode is 12 digits</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:String">
			<xs:pattern value="\d{12}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Album">
		<xs:sequence>
			<xs:element id="album_artist" name="artist" type="Artist" />
			<xs:element id="album_title" name="title" type="jadn:String" />
			<xs:element id="album_pub_data" name="pub_data" type="Publication-Data" />
			<xs:element id="album_tracks" name="tracks" type="Track" maxOccurs="unbounded" />
			<xs:element id="album_cover_art" name="cover_art" type="Cover-Art" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Artist">
		<xs:sequence>
			<xs:element id="artist_artist_name" name="artist_name" type="jadn:String" />
			<xs:element id="artist_instruments" name="instruments" type="Instrument" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Instrument">
		<xs:annotation>
			<xs:documentation>collection of instruments (non-exhaustive)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:String">
			<xs:enumeration id="instrument_1" value="vocals" />
			<xs:enumeration id="instrument_2" value="guitar" />
			<xs:enumeration id="instrument_3" value="bass" />
			<xs:enumeration id="instrument_4" value="drums" />
			<xs:enumeration id="instrument_5" value="keyboards" />
			<xs:enumeration id="instrument_6" value="percussion" />
			<xs:enumeration id="instrument_7" value="brass" />
			<xs:enumeration id="instrument_8" value="woodwinds" />
			<xs:enumeration id="instrument_9" value="harmonica" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Publication-Data">
		<xs:sequence>
			<xs:element id="publication-data_label" name="label" type="jadn:String" />
			<xs:element id="publication-data_rel_date" name="rel_date">
				<xs:simpleType>
					<xs:restriction base="jadn:date">
						<xs:annotation>
							<xs:documentation>JSON Schema Section 7.3.1</xs:documentation>
						</xs:annotation>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Track">
		<xs:sequence>
			<xs:element id="track_t_number" name="t_number" type="jadn:Number" />
			<xs:element id="track_title" name="title" type="jadn:String" />
			<xs:element id="track_length" name="length">
				<xs:simpleType>
					<xs:restriction base="jadn:time">
						<xs:annotation>
							<xs:documentation>JSON Schema Section 7.3.1</xs:documentation>
						</xs:annotation>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element id="track_featured" name="featured" type="Artist" />
			<xs:element id="track_audio" name="audio" type="Audio" />
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Audio">
		<xs:sequence>
			<xs:element id="audio_a_format" name="a_format" type="Audio-Format" />
			<xs:element id="audio_a_content" name="a_content" type="jadn:Binary" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Audio-Format">
		<xs:annotation>
			<xs:documentation>can only be one, but can extend list</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:String">
			<xs:enumeration id="audio-format_1" value="MP3" />
			<xs:enumeration id="audio-format_2" value="OGG" />
			<xs:enumeration id="audio-format_3" value="FLAC" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Cover-Art">
		<xs:sequence>
			<xs:element id="cover-art_i_format" name="i_format" type="Image-Format" />
			<xs:element id="cover-art_i_content" name="i_content" type="jadn:Binary" />
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="Image-Format">
		<xs:annotation>
			<xs:documentation>can only be one, but can extend list</xs:documentation>
		</xs:annotation>
		<xs:restriction base="jadn:String">
			<xs:enumeration id="image-format_1" value="PNG" />
			<xs:enumeration id="image-format_2" value="JPG" />
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="Choice-Name">
		<xs:choice>
			<xs:element id="choice-name_field_value_1" name="field_value_1" type="jadn:String" />
			<xs:element id="choice-name_field_value_2" name="field_value_2" type="jadn:String" />
		</xs:choice>
	</xs:complexType>
	<xs:simpleType name="String-Name">
		<xs:restriction base="jadn:ipv4">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.4</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String-Name-1">
		<xs:restriction base="jadn:ipv6">
			<xs:annotation>
				<xs:documentation>JSON Schema Section 7.3.4</xs:documentation>
			</xs:annotation>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="String-Name-2">
		<xs:restriction base="jadn:String" />
	</xs:simpleType>
	<xs:element id="Library" name="Library" type="Library" />
	<xs:element id="Album" name="Album" type="Album" />
	<xs:element id="Track" name="Track" type="Track" />
</xs:schema>