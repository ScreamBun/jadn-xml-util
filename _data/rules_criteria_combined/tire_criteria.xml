<?xml version="1.0" encoding="UTF-8"?>
<criteria:CriteriaSet xmlns:criteria="http://example.org/rules"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://example.org/rules test_criteria.xsd">

    <Criteria criteriaID="bf90c9c6-370d-4c8d-b9ef-134a624decf5" criteriaName="PassengerCarTireCriteria">
        <description>Passenger Car Tire Criteria</description>
        <criteriaValues>
            <CriteriaValue>
                <valueName>TP</valueName>
                <evalItem>
                    <valueRange>
                        <min>31</min>
                        <max>51</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>
            <CriteriaValue>
                <valueName>Temp</valueName>
                <evalItem>
                    <valueRange>
                        <min>-15</min>
                        <max>120</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>             
        </criteriaValues>
    </Criteria>
    <Criteria criteriaID="deaa7a67-1fa0-46a8-8d47-45acea9ec219" criteriaName="BicycleTireCriteria">
        <description>Bicycle Tire Criteria</description>
        <criteriaValues>
            <CriteriaValue>
                <valueName>TP</valueName>
                <evalItem>
                    <valueRange>
                        <min>40</min>
                        <max>70</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>
            <CriteriaValue>
                <valueName>Temp</valueName>
                <evalItem>
                    <valueRange>
                        <min>0</min>
                        <max>110</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>            
        </criteriaValues>
    </Criteria>    

</criteria:CriteriaSet>