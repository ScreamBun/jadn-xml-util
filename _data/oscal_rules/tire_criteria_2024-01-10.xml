    <Criteria criteriaID="1704925016376" criteriaName="PassengerCarTireCriteria">
        <description>Passenger Car Tire Criteria</description>
        <criteriaValues>
            <CriteriaValue>
                <valueName>TP</valueName>
                <evalItem>
                    <valueRange>
                        <min>31</min>
                        <max>51</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>
            <CriteriaValue>
                <valueName>Temp</valueName>
                <evalItem>
                    <valueRange>
                        <min>-15</min>
                        <max>120</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>             
        </criteriaValues>
    </Criteria>

    <Criteria criteriaID="1704925021666" criteriaName="BicycleTireCriteria">
