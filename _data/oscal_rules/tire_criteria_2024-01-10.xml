        <criteriaValues>
            <CriteriaValue>
                <valueName>TP</valueName>
                <evalItem>
                    <valueRange>
                        <min>40</min>
                        <max>70</max>
                    </valueRange>
                </evalItem>
            </CriteriaValue>
            <CriteriaValue>
                <valueName>Temp</valueName>
                <evalItem>
                    <valueRange>
                        <min>0</min>
                        <max>110</max>
